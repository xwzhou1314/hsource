<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hsource.item.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hsource.item.entity.Dict">
        <id column="id" property="id" />
        <result column="code" property="code" />
        <result column="parent_code" property="parentCode" />
        <result column="key" property="key" />
        <result column="value" property="value" />
        <result column="operate_date" property="operateDate" />
        <result column="operate_user" property="operateUser" />
        <result column="remark" property="remark" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id AS id, code AS code, parent_code AS parentCode, key AS key, value AS value, operate_date AS operateDate, operate_user AS operateUser, remark AS remark
    </sql>

</mapper>
